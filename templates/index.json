 strong {\n      font-size: 16px;\n      color: #F53A4E;\n    }\n  \n    svg {\n      display: block;\n    }\n  </style>\n  \n  <script>\n    function updateDeliveryDate() {\n      const minDays = 7; // Start of delivery range\n      const maxDays = 8; // End of delivery range\n  \n      let today = new Date();\n      let startDate = new Date(today);\n      let endDate = new Date(today);\n  \n      startDate.setDate(today.getDate() + minDays); // First delivery day\n      endDate.setDate(today.getDate() + maxDays); // Last delivery day\n  \n      const options = { day: 'numeric', month: 'long' }; // Format: \"18 February\"\n  \n      let formattedStartDate = startDate.toLocaleDateString('en-US', options);\n      let formattedEndDate = endDate.toLocaleDateString('en-US', options);\n  \n      // If the month is the same, only show the day for the second date\n      let startMonth = startDate.toLocaleDateString('en-US', { month: 'long' });\n      let endMonth = endDate.toLocaleDateString('en-US', { month: 'long' });\n  \n      let displayDate = (startMonth === endMonth)\n        ? `${startDate.getDate()}-${endDate.getDate()} ${startMonth}`\n        : `${formattedStartDate} - ${formattedEndDate}`;\n  \n      document.getElementById(\"arrival-date-text\").innerText = displayDate;\n    }\n  \n    updateDeliveryDate();\n  </script>\n          </div>\n            </div></div>"
